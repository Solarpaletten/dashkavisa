# ============================================
# visa_bot Configuration Template
# ============================================
# Copy this file to .env and fill in your values
# NEVER commit .env to Git!
# ============================================

# === Telegram Bot ===
# Get token from @BotFather
TELEGRAM_BOT_TOKEN=your_bot_token_here

# === VFS Global Credentials ===
# Your VFS Global account
VFS_EMAIL=your_email@example.com
VFS_PASSWORD=your_password_here

# === User Data (as in passport) ===
USER_FIRST_NAME=FIRSTNAME
USER_LAST_NAME=LASTNAME
USER_BIRTH_DATE=01.01.1990
USER_PASSPORT=AB1234567

# === Settings ===
CITY=Минск
VISA_TYPE=Шенген виза
CHECK_INTERVAL=60
MAX_DATES_TO_SHOW=5

# === Logging ===
LOG_LEVEL=INFO
